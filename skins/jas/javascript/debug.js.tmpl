function copyLog() {
  var log = document.getElementById("logDisplay").textContent;
  navigator.clipboard.writeText(log);
}

function clearLog() {
    var logDisplayElem = document.getElementById("logDisplay");
        logDisplayElem.innerHTML = "";
}